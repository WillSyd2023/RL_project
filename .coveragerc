[run]
branch = True
source =
    nmr_problem
    percep_filter_problem
    rl_agent
    rl_policy_eval
omit =
    */__init__.py
    */__pycache__/*
    */tests/*
    setup.py
    */run.py
    run.py
    rl_project.egg-info/*

[report]
exclude_lines =
    pragma: no cover
    @abstractmethod
ignore_errors = True
show_missing = True

[html]
directory = htmlcov